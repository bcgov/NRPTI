{
  "name": "nrpti-admin",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --base-href /admin/ --deploy-url /admin/",
    "build": "npm run lint-fix && ng build --prod --deploy-url /admin/ --base-href /admin/",
    "postinstall": "ng build --prod --deploy-url /admin/ --base-href /admin/",
    "test": "echo Test suite should be run with `npm run tests` or `npm run tests-ci`",
    "tests": "ng test",
    "tests-ci": "ng test --watch=false --code-coverage",
    "e2e": "ng e2e",
    "lint:ts": "ng lint nrpti-admin --format=stylish",
    "lint:scss": "stylelint src/**/*.scss",
    "lint": "npm-run-all -l -s -c lint:*",
    "lint-fix:ts-1": "ng lint nrpti-admin --fix --format=stylish",
    "lint-fix:ts-2": "prettier src/**/*.ts --write --loglevel warn",
    "lint-fix:ts": "npm-run-all -l -s -c lint-fix:ts-*",
    "lint-fix:scss": "stylelint src/**/*.scss --fix",
    "lint-fix": "npm-run-all -l -s -c lint-fix:ts lint-fix:scss"
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.0.0"
  },
