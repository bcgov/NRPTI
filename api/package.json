{
  "name": "nrpti-api",
  "version": "1.0.1",
  "license": "Apache-2.0",
  "main": "app",
  "scripts": {
    "start": "node app",
    "test": "node_modules/.bin/jest --verbose --forceExit",
    "test-mac": "env MONGOMS_ARCH=x64 npm run test --detectOpenHandles",
    "dev": "nodemon app.js",
    "lint": "npm-run-all -l -s -c lint:*",
    "lint:1": "eslint . --ignore-pattern 'node_modules' --ext .js",
    "lint:2": "prettier ./**/*.js --warn",
    "lint-fix": "npm-run-all -l -s -c lint-fix:*",
    "lint-fix:1": "eslint . --fix --ignore-pattern 'node_modules' --ext .js",
    "lint-fix:2": "prettier ./**/*.js --write --loglevel warn"
  },
  "engines": {
    "node": ">= 18.7.0",
    "npm": ">= 9.0.0"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.js",
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/test/**"
    ],
    "testResultsProcessor": "jest-sonar-reporter",
    "globalSetup": "./test/setup.js",
    "setupFiles": [
      "./test/setEnvVars.js",
      "./test/setupGlobals.js"
    ],
    "globalTeardown": "./test/teardown.js",
    "testEnvironment": "node",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    }
  },
  "jestSonar": {
    "reportPath": "coverage"
  },
  "dependencies": {
    "aws-sdk": "^2.1429.0",
    "axios": "^0.21.4",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0-rc.3",
    "clamav.js": "0.12.0",
    "compression": "^1.7.4",
    "csvtojson": "2.0.10",
    "db-migrate": "^0.11.13",
    "db-migrate-mongodb": "1.5.0",
    "express": "^4.19.2",
    "jwks-rsa": "1.6.0",
    "keycloak-js": "^26.2.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mongoose": "5.7.7",
    "qs": "^6.11.2",
    "swagger-tools": "0.10.4",
    "winston": "2.4.4",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "aws-sdk-mock": "^5.1.0",
    "canada": "^0.1.0",
    "database-cleaner": "1.3.0",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "~10.1.1",
    "factory-girl": "5.0.4",
    "faker": "^5.1.0",
    "jest": "~28.1.3",
    "jest-environment-node": "~28.1.3",
    "jest-sonar-reporter": "^2.0.0",
    "jsonwebtoken": "8.5.1",
    "mockingoose": "^2.15.2",
    "nock": "11.4.0",
    "nodemon": "3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "~3.7.3",
    "shelljs": "^0.8.5",
    "supertest": "^4.0.2"
  }
}
