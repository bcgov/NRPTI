{
  "name": "nrpti-angular",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -l -p -c start:*",
    "start:admin-nrpti": "ng serve admin-nrpti -o",
    "start:public-nrced": "ng serve public-nrced -o",
    "start:public-lng": "ng serve public-lng -o",
    "start-prod": "npm-run-all -l -p -c start-prod:*",
    "start-prod:admin-nrpti": "ng serve admin-nrpti --configuration production",
    "start-prod:public-nrced": "ng serve public-nrced --configuration production",
    "start-prod:public-lng": "ng serve public-lng --configuration production",
    "build": "npm-run-all -l -s -c build:library library:link build:app:*",
    "build:prod": "npm-run-all -l -s -c library:unlink clean:* build:library library:link build:app:*",
    "build:prod:only-nrpti": "npm-run-all -l -s -c library:unlink clean:* build:library library:link build:app:admin-nrpti",
    "build:app:common": "ng build common --configuration production",
    "build:app:admin-nrpti": "ng build admin-nrpti --configuration production",
    "build:app:public-nrced": "ng build public-nrced --configuration production",
    "build:app:public-lng": "ng build public-lng --configuration production",
    "build:library": "ng build global",
    "build:library:dev": "npm-run-all -l -s -c library:unlink clean:library build:library library:link",
    "library:link": "npm-run-all -l -s -c library:link:*",
    "library:link:1": "cd dist/nrpti-angular-components && npm link",
    "library:link:2": "npm link nrpti-angular-components",
    "library:unlink": "npm unlink nrpti-angular-components",
    "link:list": "npm ls -g --depth=0 --link=true",
    "library:package": "npm-run-all -l -s -c library:copy:* library:package:*",
    "library:package:1": "cd dist/nrpti-angular-components && npm pack",
    "library:publish": "npm-run-all -l -s -c library:copy:* library:publish:*",
    "library:publish:1": "npm publish projects/global",
    "library:copy": "npm-run-all -l -s -c library:copy:*",
    "library:copy:license": "copy ..\\LICENSE .\\dist\\global",
    "library:copy:readme": "copy .\\projects\\global\\README.md .\\dist\\global",
    "clean": "npm-run-all -l -s -c clean:*",
    "clean:library": "rm -fr dist/nrpti-angular-components",
    "clean:dist": "rm -fr dist",
    "test": "ng test --watch=false --code-coverage",
    "test:admin-nrpti": "ng test admin-nrpti",
    "test:public-nrced": "ng test public-nrced",
    "test:public-lng": "ng test public-lng",
    "test:common": "ng test common",
    "test:global": "ng test global",
    "test-ci:admin-nrpti": "ng run admin-nrpti:test-ci",
    "test-ci:public-nrced": "ng run public-nrced:test-ci",
    "test-ci:public-lng": "ng run public-lng:test-ci",
    "lint": "npm-run-all -l -s -c lint:ts:* lint:html",
    "lint:ts": "npm-run-all -l -s -c lint:ts:*",
    "lint:ts:1": "eslint --ext \".ts\" --ignore-pattern 'node_modules' .",
    "lint:ts:2": "prettier projects/**/*.ts --check --log-level warn",
    "lint:html": "prettier projects/**/*.html --check --log-level warn",
    "lint-fix": "npm-run-all -l -s -c lint-fix:ts:* lint-fix:html",
    "lint-fix:ts": "npm-run-all -l -s -c lint-fix:ts:*",
    "lint-fix:ts:1": "npm run lint:ts:1 -- --fix",
    "lint-fix:ts:2": "prettier projects/**/*.ts --write --log-level warn",
    "lint-fix:html": "prettier projects/**/*.html --write --log-level warn"
  },
  "private": true,
  "engines": {
    "node": ">= 18.19.1",
    "npm": ">= 9.0.0"
  },
  "dependencies": {
    "@angular/animations": "~19.2.2",
    "@angular/cdk": "~19.2.3",
    "@angular/common": "~19.2.2",
    "@angular/compiler": "~19.2.2",
    "@angular/core": "~19.2.2",
    "@angular/forms": "~19.2.2",
    "@angular/material": "~19.2.3",
    "@angular/platform-browser": "~19.2.2",
    "@angular/platform-browser-dynamic": "~19.2.2",
    "@angular/router": "~19.2.2",
    "@auth0/angular-jwt": "~3.0.0",
    "@ng-bootstrap/ng-bootstrap": "~18.0.0",
    "@tinymce/tinymce-angular": "^8.0.1",
    "async": "~3.1.0",
    "bootstrap": "~5.3.3",
    "chart.js": "^2.9.4",
    "classlist.js": "~1.1.20150312",
    "core-js": "^3.41.0",
    "guid-typescript": "^1.0.9",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "jquery": "~3.5.1",
    "keycloak-angular": "~19.0.2",
    "keycloak-js": "~26.1.4",
    "leaflet": "~1.3.1",
    "leaflet.markercluster": "~1.4.1",
    "linkify-it": "~2.2.0",
    "moment": "~2.24.0",
    "mygovbc-bootstrap-theme": "~0.4.1",
    "ng-inline-svg-2": "~15.0.0",
    "ng2-bootstrap-modal": "^1.0.1",
    "ngx-page-scroll": "~14.0.1",
    "ngx-page-scroll-core": "~14.0.1",
    "ngx-pagination": "^6.0.3",
    "ngx-toastr": "~19.0.0",
    "object-fit-images": "~3.2.4",
    "papaparse": "~5.2.0",
    "popper.js": "^1.16.1",
    "rxjs": "7.4.0",
    "tinymce": "^7.7.1",
    "topojson-client": "~3.0.1",
    "web-animations-js": "~2.3.2",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~19.2.4",
    "@angular-devkit/core": "~19.2.4",
    "@angular-devkit/schematics": "~19.2.4",
    "@angular-eslint/eslint-plugin": "^19.2.1",
    "@angular/cli": "~19.2.4",
    "@angular/compiler-cli": "~19.2.3",
    "@angular/language-service": "~19.2.3",
    "@eslint/eslintrc": "^3.3.0",
    "@types/arcgis-js-api": "~4.13.0",
    "@types/geojson": "~7946.0.7",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/leaflet": "~1.4.4",
    "@types/leaflet.markercluster": "~1.4.0",
    "@types/linkify-it": "~2.1.0",
    "@types/node": "~8.9.4",
    "@types/papaparse": "~5.0.4",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/eslint-plugin-tslint": "^7.0.2",
    "@typescript-eslint/parser": "^8.27.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.8",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-unicorn": "^50.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage": "^2.2.1",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "^0.0.36",
    "lint-staged": "~9.4.2",
    "ng-packagr": "~19.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "~3.0.3",
    "puppeteer": "^2.1.1",
    "tslint": "~5.20.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.27.0"
  }
}
